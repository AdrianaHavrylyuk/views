
@model IEnumerable<app2.Models.Product>


@{
    ViewData["Title"] = "ProductInfo";
}

<h1>ProductInfo</h1>
<table class="table">
    <tr>

        <th>Name</th>
        <th>Price</th>

    </tr>
    @foreach (var item in ViewBag.prod)
    {
        <tr>
            <td>@item.name</td>
            <td>@item.price</td>
        </tr>
    }
</table>

